{
  "name": "@alexamies/chinesedict-js",
  "version": "0.1.19",
  "description": "A JavaScript browser module for a Chinese-English dictionary",
  "license": "Apache-2.0",
  "repository": "alexamies/chinesedict-js",
  "browser": "index.js",
  "main": "@alexamies/chinesedict-js",
  "types": "./index.d.ts",
  "author": "alexamies",
  "files": [
    "src/*.*",
    "index.d.ts",
    "chinesedict.css",
    "umd/chinesedict.js"
  ],
  "scripts": {
    "build": "webpack",
    "cc-cedict": "node build/cccedict_generator.js ./cedict_ts.u8",
    "prebuild": "npm run lint && node_modules/typescript/bin/tsc",
    "generate_dict": "node build/gen_dictionary.js",
    "lint": "./node_modules/.bin/tslint -c tslint.json '*.ts' -e index.d.ts --fix",
    "start": "webpack-dev-server"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.10",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "csv-parse": "^4.4.6",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.2.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "repl": "^0.1.3",
    "sass-loader": "^8.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "yargs": "^12.0.5"
  }
}
