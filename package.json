{
  "name": "@alexamies/chinesedict-js",
  "version": "0.1.23",
  "description": "A JavaScript browser module for a Chinese-English dictionary",
  "license": "Apache-2.0",
  "repository": "alexamies/chinesedict-js",
  "browser": "index.js",
  "main": "@alexamies/chinesedict-js",
  "types": "./index.d.ts",
  "author": "alexamies",
  "files": [
    "src/*.d.ts",
    "src/*.js",
    "index.d.ts",
    "chinesedict.css",
    "umd/chinesedict.js"
  ],
  "scripts": {
    "build": "webpack",
    "cc-cedict": "node build/cccedict_generator.js ./cedict_ts.u8",
    "prebuild": "npm run lint && node_modules/typescript/bin/tsc",
    "generate_dict": "node build/gen_dictionary.js",
    "lint": "./node_modules/.bin/tslint -c tslint.json '*.ts' -e index.d.ts --fix",
    "start": "webpack-dev-server --open",
    "test": "karma start karma.conf.js"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.10",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/jasmine": "^3.5.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "csv-parse": "^4.8.3",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.3.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.1.1",
    "mocha": "^10.2.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "repl": "^0.1.3",
    "sass-loader": "^8.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "yargs": "^12.0.5"
  }
}
